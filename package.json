{
  "name": "template-project",
  "version": "1.0.0",
  "description": "A Basic template project",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node app",
    "test": "jest --coverage --detectOpenHandles"
  },
  "author": "Venkat Aniket Reddy",
  "license": "ISC",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-console": [
        "off"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2022
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    }
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "config": "^3.3.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "morgan": "^1.10.0",
    "mssql": "^10.0.2",
    "passport": "^0.7.0",
    "sequelize": "^6.37.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "eslint": "^9.3.0",
    "globals": "^15.3.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Venkataniket07/template-project.git"
  }
}
